{
  "manifest_version": 3,
  "name": "Jester",
  "version": "0.1.1",
  "description": "A web extension to fool web trackers.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
